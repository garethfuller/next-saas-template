{
  "name": "app2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "db:migrate:dev": "prisma migrate dev",
    "db:ui": "prisma studio",
    "test:unit": "vitest --config ./tests/vitest.config.unit.ts",
    "test:unit:ui": "vitest --config ./tests/vitest.config.unit.ts --ui",
    "test:int": "./lib/scripts/run-integration.sh",
    "test:int:ui": "./lib/scripts/run-integration.sh --ui",
    "test:coverage": "vitest --coverage --config ./tests/vitest.config.ts",
    "worker:start": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' lib/workers/index.ts",
    "dev:services": "docker-compose up"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.0.0",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.11.0",
    "bullmq": "^3.10.4",
    "cors": "^2.8.5",
    "next": "13.2.4",
    "next-auth": "^4.20.1",
    "nodemailer": "^6.9.1",
    "postmark": "^3.0.15",
    "raw-body": "^2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.8",
    "stripe": "^11.17.0",
    "ts-node": "^10.9.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdom": "^21.1.1",
    "prettier": "^2.8.6",
    "prisma": "^4.12.0",
    "typescript": "5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.30.1",
    "vitest-mock-extended": "^1.1.3"
  }
